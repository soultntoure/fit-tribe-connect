{
  "name": "fit-tribe-connect-backend",
  "version": "1.0.0",
  "description": "Backend API for Fit Tribe Connect",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format:check": "prettier --log-level warn --check .",
    "format:write": "prettier --log-level warn --write .",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0 --watch",
    "start:prod": "node dist/main"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    \"axios\": "^1.0.0",
    \"class-transformer\": "^0.5.1",
    \"class-validator\": "^0.14.0",
    \"jsonwebtoken\": "^9.0.0",
    \"moment\": "^2.29.4",
    \"nestjs-prisma\": "^0.6.2",
    \"pg\": "^8.11.3",
    \"pg-hstore\": "^2.3.4",
    \"reflect-metadata\": "^0.1.13",
    \"rxjs\": "^7.8.0",
    \"uuid\": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-config": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    \"eslint\": "^8.42.0",
    \"eslint-config-prettier\": "^8.8.0",
    \"eslint-plugin-prettier\": "^4.2.1",
    \"jest\": "^29.5.0",
    \"prettier\": "^2.8.8",
    \"prisma\": "^5.5.2",
    \"source-map-support\": "^0.5.21",
    \"supertest\": "^6.3.3",
    \"ts-jest\": "^29.1.0",
    \"ts-loader\": "^9.4.3",
    \"typescript\": "^5.0.4"
  },
  "prisma": {
    "client": {
      "pretty": true,
      "typescript": true,
      "introspection": {
        "cuid": true,
        "url": "env:DATABASE_URL"
      }
    }
  },
  \"author\": "",
  \"license\": "ISC"
}